# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

use_frameworks!

# Pods for all Targets
def shared_pods
    pod 'Alamofire'
    pod 'SDWebImage', '~> 5.11'
    pod 'SwiftLint'
end

target 'TheMovieDBApp' do
    shared_pods
end

target 'TheMovieDBAppTests' do
    shared_pods
end
